<!DOCTYPE html>
<html ng-app="myApp">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/app/assets/bootstrap/dist/css/bootstrap.min.css') }}"  media="screen" charset="utf-8">
        <link />
    </head>
    <body>

        <nav class="navbar navbar-default">
          <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand" href="#">Symfony-Angular</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
              <ul class="nav navbar-nav navbar-right">
                  <li role="presentation"><a href="#posts">Post</a></li>
                  <li role="presentation"><a href="#users">Users</a></li>
              </ul>
            </div><!-- /.navbar-collapse -->
          </div><!-- /.container-fluid -->
        </nav>
        <div class="container">
            <ng-view />
        </div>
        <script src="{{ asset('bundles/app/assets/jquery/dist/jquery.min.js') }}"></script>
        <script src="{{ asset('bundles/app/assets/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
        <script src="{{ asset('bundles/app/assets/angular/angular.min.js') }}"></script>
        <script src="{{ asset('bundles/app/assets/angular-route/angular-route.min.js') }}"></script>
        <script src="{{ asset('bundles/app/assets/angular-resource/angular-resource.min.js') }}"></script>
        <script src="{{ asset('bundles/app/js/app.js') }}"></script>
    </body>
</html>
